{
  "name": "ircra",
  "version": "2.2.4",
  "description": "IRCRA Climbing Grade Conversion Utility",
  "repository": {
    "type": "git",
    "url": "https://github.com/wulfmann/ircra.git"
  },
  "main": "dist/ircra.js",
  "module": "dist/ircra.mjs",
  "unpkg": "dist/ircra.umd.js",
  "source": "src/ircra.ts",
  "author": "Joe Snell <joepsnell@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "unit": "jest",
    "pretty": "prettier src/ircra.ts src/master.ts --write",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "test": "jest",
    "build": "microbundle --compress --raw",
    "dev": "microbundle watch",
    "bump": "standard-version",
    "release": "git push --follow-tags origin master"
  },
  "devDependencies": {
    "@types/jest": "^24.0.17",
    "jest": "^24.9.0",
    "microbundle": "^0.11.0",
    "prettier": "^1.18.2",
    "standard-version": "^8.0.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "keywords": [
    "rock climbing",
    "bouldering",
    "climbing grades",
    "grades",
    "ircra"
  ]
}
